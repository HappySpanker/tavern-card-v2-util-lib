{
  "compilerOptions": {
    "target": "es2015",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "outDir": "./dist",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "strict": true,
    "skipLibCheck": true
  },
  "$schema": "https://json.schemastore.org/tsconfig"
}